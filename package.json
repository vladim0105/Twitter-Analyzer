{
  "name": "twitter-analyzer",
  "version": "1.0.1",
  "description": "Analyzes Twitter Accounts",
  "icon": "res/icon.ico",
  "main": "./dist/main.js",
  "scripts": {
    "build": "tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "cross-env ELECTRON_IS_DEV=true && yarn run build && electron ./dist/main.js",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "your.id",
    "mac": {
      "category": "your.app.category.type"
    },
    "directories": {
      "output": "bin"
    },
    "productName": "Twitter Analyzer"
  },
  "devDependencies": {
    "@types/chart.js": "^2.8.9",
    "@types/jquery": "^3.3.31",
    "@types/request": "^2.48.3",
    "cross-env": "^6.0.3",
    "electron": "^9.1.0",
    "electron-builder": "^22.1.0",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "chart.js": "^2.9.1",
    "chartjs-plugin-sort": "^0.1.1",
    "eslint": "^6.6.0",
    "extract-emoji": "^1.0.2",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "request": "^2.88.0",
    "electron-is-dev": "^1.1.0"
  }
}
